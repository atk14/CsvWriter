{
        "name": "atk14/csv-writer",
        "type": "library",
        "description": "Writes CSV into string output or a file. Also provides export to XLSX format.",
        "keywords": ["csv", "export", "exporter", "writer","xlsx", "xls"],
        "homepage": "https://github.com/atk14/CsvWriter",
        "license": ["MIT"],
        "authors": [
                {
                        "name": "ATK14 Development Team",
                        "homepage": "https://github.com/orgs/atk14/people"
                }
        ],
        "require": {
                "php": ">=5.4.0",
                "ellumilel/php-excel-writer": "^0.1.9"
        },
        "require-dev": {
                "atk14/tester": "*",
                "atk14/files": "^1.4.2"
        },
        "autoload": {
                "classmap": ["src"]
        }
}
